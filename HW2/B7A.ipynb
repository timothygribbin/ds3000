    "import numpy as np\n",
    "\n",
    "A = np.array([\n",
    "    [1, 1, 1],\n",
    "    [2, 3, 1],\n",
    "    [-1, 1, 2]\n",
    "], dtype=float)\n",
    "\n",
    "b = np.array([3, 7, 2], dtype=float)\n",
    "\n",
    "solution = np.linalg.solve(A, b)\n",
    "\n",
    "print(\"Solution : \", solution)"
